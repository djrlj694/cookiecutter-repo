{
    "project_name": "___PROJECT_NAME___",
    "project_platform": ["Unspecified", "Cookiecutter", "Python", "Swift"],
    "project_type": ["Unspecified", "CLI", "Lib", "Script", "Template"],
    "__repo_platform": "{{ cookiecutter.project_platform|replace('Unspecified', '') }}",
    "repo_platform": "{{ cookiecutter.__repo_platform }}",
    "__repo_type": "{{cookiecutter.project_type | replace('Unspecified', '')}}",
    "__repo_suffix": "{{cookiecutter.project_name | replace(' ', '-') | replace('_', '-')}}",
    "__repo_name_parts": ["{{cookiecutter.__repo_platform}}", "{{cookiecutter.__repo_type}}", "{{cookiecutter.__repo_suffix}}"],
    "repo_name": "{{[cookiecutter.project_platform, cookiecutter.project_type, cookiecutter.project_name] | select('!=', '') | join('-') | lower}}",
    "repo_dir": "./{{cookiecutter.repo_name}}",
    "repo_description": "___REPO_DESCRIPTION___",
    "repo_license": ["Not open source", "Apache Software License 2.0", "BSD-3", "GNU GPL v3.0", "MIT"],
    "lead_name": "{{default_context.lead_name or '___LEAD_NAME___'}}",
    "lead_email": "{{default_context.lead_email or '___USER@DOMAIN.TLD___'}}",
    "github_user": "{{default_context.github_user or '___GITHUB_USER___'}}",
    "travis_user": "{{default_context.travis_user or '___TRAVIS_USER___'}}"
}
